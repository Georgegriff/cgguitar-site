{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "11ty",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/eleventy",
      "stopOnEntry": false,
      "args": ["--watch"],
      "cwd": "${workspaceRoot}",
      "console": "internalConsole"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nunjucks.js file",
      "program": "${workspaceFolder}/tasks/nunjucks.js",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector"
    }
  ]
}
