{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "11ty",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/eleventy",
      "stopOnEntry": false,
      "args": ["--watch"],
      "cwd": "${workspaceRoot}",
      "console": "internalConsole"
    },
    {
      "name": "Nunjucks compile",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/admin/nunjucks-compile.js",
      "stopOnEntry": false,
      "args": ["--watch"],
      "cwd": "${workspaceRoot}",
      "console": "internalConsole"
    }
  ]
}
